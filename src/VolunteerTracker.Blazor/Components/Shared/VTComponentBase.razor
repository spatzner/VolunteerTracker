

@code {

    [Parameter]
    public string Class { get; set; } = string.Empty;

    [Parameter]
    public string? Id { get; set; }

    [Parameter]
    public string Name { get; set; } = string.Empty;

    protected override void OnInitialized()
    {
        Id ??= Guid.NewGuid().ToString().Replace("-", "");
    }
}
